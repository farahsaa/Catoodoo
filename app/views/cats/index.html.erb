<%= link_to("newcat", new_cat_path, class: "btn btn-primary float-right") %>

<table class="table">
  <div class="container">
      <h1>ABOUT THE CATS</h1>
      <p>ALL THE CATS</p>
  </div>
  <thead>
    <tr>
      <th scope="col">Name </th>
    </tr>
  </thead>
  <% @cats.each do |cat| %>
    <tr>
      <td>
        <%= link_to(cat.name, cat_path(cat)) %>
      </td>
    </tr>
  <% end %>
</table>



