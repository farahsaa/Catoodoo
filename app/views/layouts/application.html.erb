<!DOCTYPE html>
<html>
  <head>
    <title>Todolist</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <ul>
      <li>
        <%= link_to('tasks', tasks_path) %>
      </li>
      <li>
        <%= link_to('cats', cats_path) %>
      </li>
    </ul>
    <%= yield %>
  </body>
</html>
